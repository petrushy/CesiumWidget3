{
  "name": "CesiumWidget3",
  "version": "0.1.0",
  "description": "A Custom Jupyter Widget Library",
  "author": "Petrus Hyv√∂nen",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com//CesiumWidget3.git"
  },
  "resolutions": {
    "@babel/runtime": "^7.12.5"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/",
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf dist/ && rimraf ../cesiumwidget3/labextension/ && rimraf ../cesiumwidget3/nbextension",
    "prepublish": "yarn run clean && yarn run build:prod",
    "build": "webpack --mode=development && yarn run build:labextension:dev",
    "build:prod": "webpack --mode=production && yarn run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "watch": "webpack --watch --mode=development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/helper-compilation-targets": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@jupyterlab/builder": "^3.0.0",
    "babel-loader": "^8.2.2",
    "cesium": "^1",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.2",
    "html-webpack-include-assets-plugin": "^2.0.0",
    "html-webpack-plugin": "^4.5.1",
    "html-webpack-tags-plugin": "^3.0.0",
    "json-loader": "^0.5.7",
    "rimraf": "^2.6.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.1 || ^2 || ^3 || ^4",
    "cesium": "^1.78.0",
    "file-loader": "^6.2.0",
    "lodash": "^4.17.4"
  },
  "jupyterlab": {
    "extension": "lib/labplugin",
    "outputDir": "../cesiumwidget3/labextension",
    "webpackConfig": "./webpack-additions.config.js",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    }
  }
}
